{
    "name": "Hugo Development Container",
    "image": "huxulm69/devimgs:hugo-debian-latest",
    "runArgs": [
        "--privileged",
        "--network=host"
    ],
    "containerEnv": {
        "http_proxy": "${localEnv:http_proxy}",
        "https_proxy": "${localEnv:https_proxy}",
        "no_proxy": "${localEnv:no_proxy}"
    }
}